{
    "multipart": [
        {
            "apply": {
                "model": "encircled:block/pillar/withered_stone_pillar"
            }
        },
        {
            "when": {
                "has_top": "false"
            },
            "apply": {
                "model": "encircled:block/pillar/withered_stone_pillar_top"
            }
        },
        {
            "when": {
                "has_bottom": "false"
            },
            "apply": {
