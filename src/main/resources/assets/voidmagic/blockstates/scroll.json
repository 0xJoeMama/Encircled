{
    "multipart": [
        {
            "when": {
                "open": "false",
                "facing": "north"
            },
            "apply": {
                "model": "voidmagic:block/scroll/closed_scroll",
                "y": 90
            }
        },
        {
            "when": {
                "open": "false",
                "facing": "east"
            },
            "apply": {
                "model": "voidmagic:block/scroll/closed_scroll",
                "y": 180
            }
        },
        {
            "when": {
                "open": "false",
                "facing": "south"
            },
            "apply": {
                "model": "voidmagic:block/scroll/closed_scroll",
                "y": 270
            }
        },
        {
            "when": {
                "open": "false",
                "facing": "west"
            },
            "apply": {
                "model": "voidmagic:block/scroll/closed_scroll"
            }
        },
        {
            "when": {
                "open": "true",
                "facing": "north"
            },
            "apply": {
                "model": "voidmagic:block/scroll/open_scroll",
                "y": 90
            }
        },
        {
            "when": {
                "open": "true",
                "facing": "east"
            },
            "apply": {
                "model": "voidmagic:block/scroll/open_scroll",
                "y": 180
            }
        },
        {
            "when": {
                "open": "true",
                "facing": "south"
            },
            "apply": {
                "model": "voidmagic:block/scroll/open_scroll",
                "y": 270
            }
        },
        {
            "when": {
                "open": "true",
                "facing": "west"
            },
            "apply": {
                "model": "voidmagic:block/scroll/open_scroll"
            }
        }
    ]
}